<!DOCTYPE html> <html ng-app="loginApp"> <head><script
src="https://ajax.googleapis.com/ajax/libs/angularjs/1.8.2/angular.min.js"
></script>
</head> <body ng-controller="loginController"> <h1>Login Form</h1>
<form ng-submit="login()">
Username<br> <input type="text" ng-model="username" required> <br> Password <br> <input type="password" ng-model="password" required> <br> <button type="submit">Login</button>
</form> <script>
var app = angular.module('loginApp', []);
app.controller('loginController', function ($scope) {
$scope.login = function () {
if ($scope.username.toLowerCase() === 'viraj' && 
$scope.password === '123') {
alert('Login successful');
} else {
alert('Login failed. Invalid username or password.');
}
};
});
</script>
</body>
</html>
